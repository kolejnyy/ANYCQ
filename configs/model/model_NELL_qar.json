{
    "hidden_dim": 128,
    "aggr_val2cst": "max",
    "aggr_cst2val": "max",
    "aggr_val2var": "max",
    "sampling": "global",
    "T_train": 12,
    "T_val": 200,
    "epochs": 500,
    "epoch_steps": 1000,
    "val_interval": 1,
    "lr": 5e-06,
    "lr_decay": 0.1,
    "weight_decay": 0.0,
    
    "batch_size": 4,
    "val_batch_size": [60, 30, 20, 20, 20],
    
    "loss_config": {
        "discount": 0.75,
        "reward": "improve"
    },

    "val_data": {
        "folder_path": 	"data/NELL-QAR",
        
        "data_files":	[
            "qar_valid_3piv_efo1.json",
            "qar_valid_3piv_efo1.json",
            "qar_valid_3piv_efo1.json",
            "qar_valid_3piv_efo1.json"
        ],
        "data_file": "",
        
        "n_relations": 400,
        "n_entities": 63361,
        
        "predictor": {
            "type" : "SignCQPred",
            "predictor": {
                "type": "ComplEx",
                "load_path": "models/NELL-EFO1/predictor/ComplEx_pretrained.pth",
                
                "tau": 5,

                "n_relations": 400,
                "n_entities": 63361,
                "embedding_dim": 1000
            },

            "perfect": {
                "type": "PerfeCT",
                "graph_path": "data/NELL-EFO1/valid_kg.tsv"
            },

            "scaling_rule": "qto",
            "eps": 0.001,
            "threshold": 0.5,
            
            "temperature": 1.0,
            "temperatures":  [0.5, 1.0, 2.0, 20.0],

            "PE_path": "models/NELL-EFO1/reasoner_t1.0/qto/",
            "PE_paths": [
                "models/NELL-EFO1/reasoner_t0.5/qto/",
                "models/NELL-EFO1/reasoner_t1.0/qto/",
                "models/NELL-EFO1/reasoner_t2.0/qto/",
                "models/NELL-EFO1/reasoner_t20.0/qto/"
            ]
        }    
    },

    "test_data": {
        "folder_path": 	"data/NELL-QAR",
        
        "data_files":	[
            "qac_test_3piv_efo1.json",
            "qac_test_3piv_efo2.json",
            "qac_test_3piv_efo3.json"
        ],
        "data_file": "",
        
        "n_relations": 400,
        "n_entities": 63361,
        
        "predictor": {
            "type" : "BinCQPred",
            "predictor": {
                "type": "ComplEx",
                "load_path": "models/NELL-EFO1/predictor/ComplEx_pretrained.pth",
                
                "tau": 5,

                "n_relations": 400,
                "n_entities": 63361,
                "embedding_dim": 1000
            },

            "perfect": {
                "type": "PerfeCT",
                "graph_path": "data/NELL-EFO1/valid_kg.tsv"
            },

            "scaling_rule": "qto",
            "eps": 0.001,
            "threshold": 0.5,
            
            "temperature": 20.0,
            "temperatures":  [20.0, 20.0, 20.0, 20.0, 20.0, 20.0],

            "PE_path": "models/NELL-EFO1/reasoner_test_20.0/qto/",
            "PE_paths": [
                "models/NELL-EFO1/reasoner_test_20.0/qto/",
                "models/NELL-EFO1/reasoner_test_20.0/qto/",
                "models/NELL-EFO1/reasoner_test_20.0/qto/",
                "models/NELL-EFO1/reasoner_test_20.0/qto/",
                "models/NELL-EFO1/reasoner_test_20.0/qto/",
                "models/NELL-EFO1/reasoner_test_20.0/qto/"
            ]
        }    
    }
}
